package inlämning4;

import java.util.Scanner;

public class Continue {

	boolean bool() {
		String exit;

		Scanner scan = new Scanner(System.in);

		System.out.println("vill du fortsätta? (J / N)?");
		exit = scan.nextLine();
		if (exit.contentEquals("J")) {
			return true;
		} else {
			return false;
			
		}
		
	}
}
