package ovningar;
import java.util.Scanner;
public class Ovning325 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Skriv ett namn");
		Scanner namn = scan.nextline();

		namn = namn.charAt(% 2);
		System.out.println(namn);
		
		scan.close();
		
		
	}

}
