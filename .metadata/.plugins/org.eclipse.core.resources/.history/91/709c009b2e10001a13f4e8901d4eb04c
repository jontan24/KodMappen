package inlämning4;

import java.util.Scanner;

public class Continue {
	Account j = new Account();

	 	public boolean bool() {
		String exit;

		Scanner scan = new Scanner(System.in);

		System.out.println("vill du fortsätta? (J / N)?");
		exit = scan.nextLine();
		if (exit.contentEquals("J")) {
			return true;}
			
		 else {
			return false;
			
		}
		
	}
}

